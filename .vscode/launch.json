{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Demo",
            "type": "cppdbg",
            "MIMode": "gdb",
            "miDebuggerPath": "gdb",
            "request": "launch",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "Build: SeayonDemo",
            "program": "${workspaceFolder}/bin/SeayonDemo/SeayonDemo.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
        },
        {
            "name": "Mnist",
            "type": "cppdbg",
            "MIMode": "gdb",
            "miDebuggerPath": "gdb",
            "request": "launch",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "Build: SeayonMnist",
            "program": "${workspaceFolder}/bin/SeayonMnist/SeayonMnist.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
        }
    ]
}